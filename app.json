{
  "expo": {
    "name": "ARNavigation",
    "slug": "ARNavigation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.arnavigation",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location services to provide navigation.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location services to provide navigation even when in background.",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.yourcompany.arnavigation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}